{
  "name": "oauth-wrapper-lib",
  "version": "1.0.0",
  "description": "This is a multipurpose oauth wrapper library, supporting varios oauth providers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./providers": {
      "import": "./dist/providers/index.mjs",
      "require": "./dist/providers/index.js"
    },
    "./react": {
      "import": "./dist/react/react.mjs",
      "require": "./dist/react/react.js"
    },
    "./express": {
      "import": "./dist/express/expressHelper.mjs",
      "require": "./dist/express/expressHelper.js"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SinghYuvraj0506/Oauth-Wrapper-library.git"
  },
  "keywords": [
    "Oauth",
    "Authentication",
    "Google",
    "Github"
  ],
  "author": {
    "name": "Yuvraj Singh",
    "email": "singhyuvraj0506@gmail.com",
    "url": "https://www.yuvrajsingh.info"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SinghYuvraj0506/Oauth-Wrapper-library/issues"
  },
  "homepage": "https://github.com/SinghYuvraj0506/Oauth-Wrapper-library#readme",
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@types/react": "^19.0.1",
    "react": "^19.0.0",
    "react-router-dom": "^7.0.2"
  },
  "optionalDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
